{
    "version":2,
    "builds":[
        {
            "src": "sharanblog/sharanblog/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.1" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "sharanblog/sharanblog/wsgi.py"
        }
    ]
}